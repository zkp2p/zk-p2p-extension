[
  {
    "url": "https://wise.com/gateway/v1/payments",
    "targetUrl": "https://wise.com/account/payments",
    "method": "GET",
    "type": "xmlhttprequest",
    "title": "ZKP2P Wise Registration (All Users)",
    "description": "Notarize ownership of a wise tag. To start, click the button to view your account page",
    "secretResponseSelector": [
      "(?<=\"title\":\"Account details\",\"modules\":\\[)[^\\]]+"
    ],
    "valueTransform": ["\"modules\":\"%s\""]
  },
  {
    "url": "https://wise.com/gateway/v3/profiles/\\d+/transfers/\\d+",
    "targetUrl": "https://wise.com/all-transactions?direction=OUTGOING",
    "method": "GET",
    "type": "xmlhttprequest",
    "title": "ZKP2P Wise Onramp + Registration for Offramper",
    "description": "Notarize your payment transaction. To start, click the button and identify the correct payment transaction",
    "secretResponseSelector": [
      "(?<=\"userId\":)\\d+"
    ],
    "valueTransform": [
      "\"userId\":%s"
    ]
  },
  {
    "url": "https://api.venmo.com/graphql",
    "targetUrl": "https://account.venmo.com/?feed=mine",
    "method": "GET",
    "type": "xmlhttprequest",
    "title": "ZKP2P Venmo Registration",
    "description": "Notarize ownership of a Venmo ID. To start, click the button",
    "secretResponseSelector": [
      "(?<=\"shippingAddresses\":\\[)[^\\]]+",
      "(?<=\"userBalance\":{\"value\":)\\d+.\\d+"
    ],
    "valueTransform": [
      "\"shippingAddresses\":\"%s\"",
      "\"value\":\"%s\""
    ]
  },
  {
    "url": "https://account.venmo.com/api/stories\\?feedType=me&externalId=\\d+",
    "targetUrl": "https://account.venmo.com/?feed=mine",
    "method": "GET",
    "type": "xmlhttprequest",
    "title": "ZKP2P Venmo Transfer",
    "description": "Notarize your payment transaction. To start, click the button",
    "secretResponseSelector": [],
    "valueTransform": []
  }
]