[
  {
    "url": "https://wise.com/gateway/v1/payments",
    "targetUrl": "https://wise.com/account/payments",
    "method": "GET",
    "type": "xmlhttprequest",
    "title": "ZKP2P Wise Registration (All Users)",
    "description": "Notarize ownership of a wise tag. To start, click the button to view your account page",
    "secretResponseSelector": [
      "(?<=\"title\":\"Account details\",\"modules\":\\[)[^\\]]+"
    ],
    "valueTransform": ["\"modules\":\"%s\""]
  },
  {
    "url": "https://wise.com/gateway/v3/profiles/\\d+/transfers/\\d+",
    "targetUrl": "https://wise.com/all-transactions?direction=OUTGOING",
    "method": "GET",
    "type": "xmlhttprequest",
    "title": "ZKP2P Wise Onramp + Registration for Offramper",
    "description": "Notarize your payment transaction. To start, click the button and identify the correct payment transaction",
    "secretResponseSelector": [
      "(?<=\"userId\":)\\d+"
    ],
    "valueTransform": [
      "\"userId\":%s"
    ]
  },
  {
    "url": "https://secure.chase.com/svc/rr/payments/secure/v1/quickpay/payment/activity/list",
    "targetUrl": "https://secure.chase.com/web/auth/dashboard#/dashboard/paymentsAdmin/manageRecipient/updateContacts",
    "method": "POST",
    "type": "xmlhttprequest",
    "title": "ZKP2P Chase Zelle Onramp (Part 1)",
    "description": "Notarize your payment transaction. To start, click the button and identify the correct payment transaction",
    "secretResponseSelector": [
      "(?<=\"accountName\":\")[^\"]+",
      "(?<=\"recipientName\":\")[^\"]+",
      "(?<=\"recipientId\":)\\d+"
    ],
    "valueTransform": [
      "\"accountName\":%s",
      "\"recipientName\":%s",
      "\"userId\":%s"
    ]
  },
  {
    "url": "https://secure.chase.com/svc/rr/payments/secure/v1/quickpay/payment/activity/detail/list",
    "targetUrl": "https://secure.chase.com/web/auth/dashboard#/dashboard/paymentsAdmin/manageRecipient/updateContacts",
    "method": "POST",
    "type": "xmlhttprequest",
    "title": "ZKP2P Chase Zelle Onramp (Part 2)",
    "description": "Notarize your payment transaction. To start, click the button and identify the correct payment transaction",
    "secretResponseSelector": [
      "(?<=\"fundingAccountName\":\")[^\"]+",
      "(?<=\"recipientName\":\")[^\"]+"
    ],
    "valueTransform": [
      "\"fundingAccountName\":%s",
      "\"recipientName\":%s"
    ]
  },
  {
    "url": "https://secure.chase.com/svc/rr/payments/secure/gateway/quick-pay/customer/profile/digital-social-aliases/v1/social-aliases-profiles",
    "targetUrl": "https://secure.chase.com/web/auth/dashboard#/dashboard/singleDoor/quickPaySettings/index",
    "method": "POST",
    "type": "xmlhttprequest",
    "title": "ZKP2P Chase Zelle Onramper Registration",
    "description": "Notarize ownership of a Chase Zelle display name. To start, click the button to view your account page",
    "secretResponseSelector": [
      "(?<=\"legalEntityName\":)[^,]+",
      "(?<=\"displayName\":)[^,]+",
      "(?<=\"socialAliasDetail\":\\[)[^\\]]+",
      "(?<=\"accountDetail\":\\[)[^\\]]+"
    ],
    "valueTransform": [
      "\"legalEntityName\":%s",
      "\"displayName\":%s",
      "\"socialAliasDetail\":%s",
      "\"accountDetail\":%s"
    ]
  }
]