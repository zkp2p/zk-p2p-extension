[
  {
    "url": "https://wise.com/gateway/v1/payments",
    "targetUrl": "https://wise.com/account/payments",
    "method": "GET",
    "type": "xmlhttprequest",
    "title": "Register",
    "description": "Generate proof of a Wise account required to complete registration",
    "secretResponseSelector": [
      "(?<=\"title\":\"Account details\",\"modules\":\\[)[^\\]]+"
    ],
    "metaDataSelector": [
      "(?<=\"name\":\"Your Wisetag\",\"description\":\")[^\"]+"
    ],
    "valueTransform": [
      "\"description\":%s"
    ]
  },
  {
    "url": "https://wise.com/gateway/v3/profiles/\\d+/transfers/\\d+",
    "targetUrl": "https://wise.com/all-transactions?direction=OUTGOING",
    "method": "GET",
    "type": "xmlhttprequest",
    "title": "Deposit",
    "description": "Generate proof of a past Wise transfer required to enable deposits",
    "secretResponseSelector": [
      "(?<=\"userId\":)\\d+"
    ],
    "metaDataSelector": [
      "(?<=\"targetAmount\":)\\d+",
      "(?<=\"targetCurrency\":\")[^\"]+",
      "(?<=\"state\":\"OUTGOING_PAYMENT_SENT\",\"date\":)\\d+",
      "(?<=\"state\":\")[^\"]+",
      "(?<=\"actor\":\")[^\"]+"
    ],
    "valueTransform": [
      "\"targetAmount\":%s",
      "\"targetCurrency\":%s",
      "\"date\":%s",
      "\"state\":%s",
      "\"actor\":%s"
    ]
  },
  {
    "url": "https://wise.com/gateway/v3/profiles/\\d+/transfers/\\d+",
    "targetUrl": "https://wise.com/all-transactions?direction=OUTGOING",
    "method": "GET",
    "type": "xmlhttprequest",
    "title": "On-Ramp",
    "description": "Generate proof of your completed Wise transfer to complete onramp",
    "secretResponseSelector": [
      "(?<=\"userId\":)\\d+"
    ],
    "metaDataSelector": [
      "(?<=\"targetAmount\":)([\\d.]+)",
      "(?<=\"targetCurrency\":\")[^\"]+",
      "(?<=\"state\":\"OUTGOING_PAYMENT_SENT\",\"date\":)\\d+",
      "(?<=\"state\":\")[^\"]+",
      "(?<=\"actor\":\")[^\"]+"
    ],
    "valueTransform": [
      "\"targetAmount\":%s",
      "\"targetCurrency\":%s",
      "\"date\":%s",
      "\"state\":%s",
      "\"actor\":%s"
    ]
  }
]