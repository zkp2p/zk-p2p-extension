[
  {
    "url": "https://wise.com/gateway/v1/payments",
    "targetUrl": "https://wise.com/account/payments",
    "method": "GET",
    "type": "xmlhttprequest",
    "title": "ZKP2P Wise Registration (All Users)",
    "description": "Notarize ownership of a wise tag. To start, click the button to view your account page",
    "secretResponseSelector": [
      "(?<=\"title\":\"Account details\",\"modules\":\\[)[^\\]]+"
    ],
    "valueTransform": ["\"modules\":\"%s\""]
  },
  {
    "url": "https://wise.com/gateway/v3/profiles/\\d+/transfers/\\d+",
    "targetUrl": "https://wise.com/all-transactions?direction=OUTGOING",
    "method": "GET",
    "type": "xmlhttprequest",
    "title": "ZKP2P Wise Onramp + Registration for Offramper",
    "description": "Notarize your payment transaction. To start, click the button and identify the correct payment transaction",
    "secretResponseSelector": [
      "(?<=\"userId\":)\\d+"
    ],
    "valueTransform": [
      "\"userId\":%s"
    ]
  },
  {
    "url": "https://app.revolut.com/api/retail/user/current",
    "targetUrl": "https://app.revolut.com/home",
    "method": "GET",
    "type": "xmlhttprequest",
    "title": "ZKP2P Revolut Offramper Registration",
    "description": "Notarize ownership of a Revtag. To start, click the button to view your account page",
    "secretResponseSelector": [
      "(?<=\"address\":{)[^}]+",
      "(?<=\"birthDate\":\\[)[^\\]]+",
      "(?<=\"firstName\":)[^,]+",
      "(?<=\"lastName\":)[^,]+",
      "(?<=\"middleName\":)[^,]+",
      "(?<=\"phone\":)[^,]+",
      "(?<=\"email\":)[^,]+",
      "(?<=\"ssn\":)[^,]+",
      "(?<=\"taxResidencies\":\\[)[^\\]]+",
      "(?<=\"identificationNumbers\":\\[)[^\\]]+"
    ],
    "valueTransform": [
      "\"address\":%s",
      "\"birthDate\":%s",
      "\"firstName\":%s",
      "\"lastName\":%s",
      "\"middleName\":%s",
      "\"phone\":%s",
      "\"email\":%s",
      "\"identityDetails\":%s"
    ]
  },
  {
    "url": "https://app.revolut.com/api/retail/transaction/.*",
    "targetUrl": "https://app.revolut.com/home",
    "method": "GET",
    "type": "xmlhttprequest",
    "title": "ZKP2P Revolut Onramp",
    "description": "Notarize your payment transaction. To start, click the button and identify the correct payment transaction",
    "secretResponseSelector": [
      "(?<=\"balance\":)\\d+",
      "(?<=\"description\":)[^,]+",
      "(?<=\"firstName\":)[^,]+",
      "(?<=\"lastName\":)[^,]+",
      "(?<=\"country\":)[^,]+",
      "(?<=\"params\":\\[)[^\\]]+"
    ],
    "valueTransform": [
      "\"balance\":%s",
      "\"description\":%s",
      "\"firstName\":%s",
      "\"lastName\":%s",
      "\"country\":%s",
      "\"params\":%s"
    ]
  }
]